	<skill id="10801" levels="5" name="Targeting">
	
	<!-- WesteroS
	Уменьшает устойчивость цели к атакам из лука/арбалета на 40%, Маг. Защ. на 5%.
	-->
	
		<table name="#magicLevel">85 87 92 95 98</table>
		<table name="#mDef">0.95 0.9 0.85 0.80 0.75</table>
		<table name="#bow">0.6 0.55 0.5 0.45 0.4</table>
		<set name="magicLvl" val="#magicLevel" />
		<set name="target" val="TARGET_ONE" />
		<set name="skillType" val="PDAM" />
		<set name="operateType" val="OP_ACTIVE" />
		<set name="isDebuff" val="true" />
		<for>
			<effect name="Debuff" time="10" val="0" effectPower="40" effectType="DEBUFF" stackOrder="1">
				<mul order="0x30" stat="bowWpnRcpt" val="#bow" />
				<mul order="0x30" stat="crossbowWpnRcpt" val="#bow" />
				<mul order="0x30" stat="mDef" val="#mDef" />
			</effect>
		</for>
	</skill>
    <skill id="10802" levels="1" name="Сосредоточенное Нацеливание" > 
	
	<!-- WesteroS
	Атакует цель и врагов вокруг, 
	снижает устойчивость к атакам из лука/арбалета на 60%, Маг. Защ. на 25%.
	Через определенное время цель взрывается, получая большой урон.
	-->
	
		<set name="magicLevel" val="85" />
		<set name="power" val="150"/>
		<set name="target" val="TARGET_AURA" />
		<set name="activateRate" val="60" />
		<set name="skillType" val="PDAM" />
		<set name="operateType" val="OP_ACTIVE" />
		<for>
		<effect name="Debuff" time="10" val="0" effectPower="40" effectType="DEBUFF" stackOrder="1">
			<mul order="0x30" stat="bowWpnRcpt" val="0.6" />
			<mul order="0x30" stat="crossbowWpnRcpt" val="0.6" />
			<mul order="0x30" stat="mDef" val="0.75" />
		</effect>
		</for>
	</skill>
