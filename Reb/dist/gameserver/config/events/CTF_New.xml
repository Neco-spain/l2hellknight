<?xml version='1.0' encoding='utf-8'?>
<list>
	<!-- В шапке эвента указываем время начала (минуты и часы), а третья переменная - это время длительность эвента в СЕКУНДАХ -->
	<ctf hour="00" min="56" TimeToEvent="120">  
	
		<!-- Минимальное и максимальное кол-во игроков для старта эвента -->
		<Participants min="1" max="20" />
		
		<!-- Минимальное и максимальный уровень для участия на эвенте -->
		<Level min="1" max="85" />

		<!-- Время на регистрацию (в секундах) -->
		<TimeToRegistration val="30" />
		
		<AllowKillBonus val="false"/>
		<!-- ID Награды за каждое убийство и её кол-во -->
		<KillReward id="57" count="80000"/>
		<!-- ID Награды по окончанию и её кол-во -->
		<!-- количество значений полей "id" и "count" дожны совпадать!!! Пишем через "," -->
		<Reward id="57,4037" count="8000000,12"/>

		<!-- Включение/Выключение нужного для участия итема(по умолчанию false(выключено)) -->
		<!-- ID Нужного для участия итема и его кол-во -->
		<AllowTakeItems val="false" />
		<TakeItems id="57" count="100000" />
		
		<!-- Снимать ли эффекты (баффы) при телепортации на эвенты. -->
		<StopAllEffects val="true" />

		<ListMageMagSupport val="1517,1518,1519,1535,1536,1537,1538"/>
		<ListMageFaiterSupport val="1499,1500,1501,1502,1503,1504,1517"/>
		
		<!-- Минимальное кол-во награды, для получения приза (в случае выигрыша). -->
		<RewardForKill val="1" />
		
		<!-- Прошу заметить, что этот эвент ограничен лишь двумя командами,
			 по той причине, что большее кол-во команд будет создавать хаус
			 на эвенте, что сделает его не популярным -->
		<TeamCoords name="Devil"/>
		<TeamCoords name="Angel"/>
		
		<!-- Координаты спавна флагов указываем в том же порядке, что и координаты,
		     спавна команд. -->
		<FlagCords x="150392" y="46728" z="-3437" h="32768" />
		<FlagCords x="148632" y="46728" z="-3437" h="0" />
		
		<!-- Время между началом боя и телепортации/началом раунда (В секундах)-->
		<PauseTime val="15" />
		
		<!-- Сколько флагов необходимо для победы (Техническая победа) -->
		<NeedFlags val="5" />
	</ctf>
	
	<ctf hour="17" min="51" TimeToEvent="120">  
	
		<!-- Минимальное и максимальное кол-во игроков для старта эвента -->
		<Participants min="1" max="20" />
		
		<!-- Минимальное и максимальный уровень для участия на эвенте -->
		<Level min="1" max="85" />

		<!-- Время на регистрацию (в секундах) -->
		<TimeToRegistration val="30" />
		
		<!-- ID Награды и её кол-во -->
		<Reward id="4037" count="10" />
		
		<!-- 
			OpenDoor и CloseDoor - позволяет закрывать или открывать двери,
			после начала эвента, неограниченное кол-во.
		-->
		<!--<OpenDoor id="00000000" />-->
		<CloseDoor id="24190001" />
		<CloseDoor id="24190002" />
		<CloseDoor id="24190003" />
		<CloseDoor id="24190004" />
		
		<!-- Снимать ли эффекты (баффы) при телепортации на эвенты. -->
		<StopAllEffects val="true" />
		
		<!-- Минимальное кол-во убийств, для получения награды (в случае выигрыша). -->
		<RewardForKill val="1" />
		
		<!-- Прошу заметить, что этот эвент ограничен лишь двумя командами,
			 по той причине, что большее кол-во команд будет создавать хаус
			 на эвенте, что сделает его не популярным -->
		<TeamCoords name="Devil" x="150392" y="46728" z="-3437" h="32768"/>
		<TeamCoords name="Angel" x="148632" y="46728" z="-3437" h="0"/>
		
		<!-- Координаты спавна флагов указываем в том же порядке, что и координаты,
		     спавна команд. -->
		<FlagCords x="150392" y="46728" z="-3437" h="32768" />
		<FlagCords x="148632" y="46728" z="-3437" h="0" />
		
		<!-- Время между началом боя и телепортации/началом раунда (В секундах)-->
		<PauseTime val="15" />
		
		<!-- Сколько флагов необходимо для победы (Техническая победа) -->
		<NeedFlags val="5" />
	
	</ctf>
	
</list>