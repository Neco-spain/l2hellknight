		<set name="skillType" val="BUFF" />
		<set name="operateType" val="OP_ACTIVE" />
		<for>
			<effect count="#lvl1" name="HealOverTime" time="1" val="25000" />
			<effect count="#lvl2" name="Buff" time="300" val="0">
				<add order="0x40" stat="runSpd" val="66" />
			</effect>
			<effect count="#lvl3" name="Buff" time="300" val="0">
				<mul order="0x30" stat="pAtkSpd" val="1.33" />
