	<skill id="11183" levels="1" name="Великая Перезарядка">
	
	<!-- WesteroS
	Увеличивает ИНТ на 2 и уменьшает время перезарядки умений на 25%.
	-->
	
		<set name="magicLevel" val="85" />
		<set name="operateType" val="OP_PASSIVE" />
		<set name="skillType" val="BUFF" />
		<set name="target" val="TARGET_SELF" />
		<for>
			<add order="0x40" stat="INT" val="2" />
			<mul order="0x30" stat="mReuse" val="0.75" />
		</for>
	</skill>
	<skill id="11184" levels="1" name="Великий Колдовской Рык">
	
