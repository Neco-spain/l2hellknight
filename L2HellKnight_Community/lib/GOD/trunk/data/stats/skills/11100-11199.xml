	<skill id="11187" levels="1" name="Великий Свет Разума">
	
	<!-- WesteroS
	Увеличивает ИНТ на 2. Также увеличивает бонус восстановления MP - если персонаж сидит - на 5.1, во время ходьбы - на 6.5.
	-->
	
		<table name="#regMpSit">5.1</table>
		<table name="#regMpWalk">6.5</table>
		<table name="#magicLevel">85</table>
		<set name="magicLevel" val="#magicLevel" />
		<set name="operateType" val="OP_PASSIVE" />
		<set name="skillType" val="BUFF" />
		<set name="target" val="TARGET_SELF" />
		<enchant1 levels="10" name="power01">
			<table name="#regMpSit">5.4 5.7 6 6.3 6.7 7 7.3 7.6 7.9 8.2</table>
			<table name="#regMpWalk">6.9 7.3 7.7 8.1 8.5 8.9 9.3 9.7 10.1 10.5</table>
		</enchant1>
		<for>
			<add order="0x40" stat="INT" val="2" />
			<add order="0x40" stat="regMp" val="#regMpSit">
				<and>
					<player resting="true" />
				</and>
			</add>
			<add order="0x40" stat="regMp" val="#regMpWalk">
				<and>
					<player moving="true" />
					<player running="false" />
				</and>
			</add>
		</for>
	</skill>
