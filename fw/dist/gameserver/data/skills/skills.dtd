<!ELEMENT list (skill)*>
<!ELEMENT skill (set|effect)*>
<!ATTLIST skill
        id CDATA #REQUIRED
        level CDATA #REQUIRED
        name CDATA #REQUIRED>
<!ELEMENT set (#PCDATA)>
<!ATTLIST set
        name CDATA #REQUIRED
        val CDATA #REQUIRED>
<!ELEMENT effect (def)*>
<!ATTLIST effect
        name CDATA #REQUIRED
        time CDATA #IMPLIED>
<!ELEMENT def (#PCDATA)>
<!ATTLIST def
        name CDATA #REQUIRED
        val CDATA #REQUIRED>
