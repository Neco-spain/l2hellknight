        <set name="hitCancelTime" val="500"/>
        <set name="mpConsume1" val="#mpConsume1"/>
        <set name="mpConsume2" val="#mpConsume2"/>
        <set name="target" val="TARGET_MENTEE"/>
        <set name="skillType" val="BUFF"/>
